{
  "name": "onemanonelaptop/nebula-project",
  "description": "Project template for the Nebula Drupal 8 profile",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "php": ">=7.2",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.7",
    "drupal/address": "^1.8",
    "drupal/admin_toolbar": "^2.2",
    "drupal/anchor_link": "^2.4",
    "drupal/better_exposed_filters": "^5",
    "drupal/bootstrap": "^3.23",
    "drupal/ckeditor_liststyle": "^1.4",
    "drupal/config_ignore": "^2.3",
    "drupal/config_readonly": "^1.0@beta",
    "drupal/console": "~1.0",
    "drupal/core-composer-scaffold": "^9",
    "drupal/core-project-message": "^9",
    "drupal/core-recommended": "^9",
    "drupal/domain_registration": "^1.4",
    "drupal/easy_breadcrumb": "^1.13",
    "drupal/email_registration": "^1.1",
    "drupal/examples": "^3.0",
    "drupal/field_group": "3.x-dev",
    "drupal/fontawesome": "^2.17",
    "drupal/formdazzle": "^2.1",
    "drupal/fullcalendar_view": "^5.0",
    "drupal/google_analytics": "^3.1",
    "drupal/image_widget_crop": "^2.3",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/link_class": "^2.0",
    "drupal/login_destination": "^2.0@beta",
    "drupal/masquerade": "^2.0@beta",
    "drupal/nebula": "1.x-dev",
    "drupal/paragraphs": "^1.12",
    "drupal/paragraphs_accordion": "1.x-dev",
    "drupal/paragraphs_wysiwyg": "1.x-dev",
    "drupal/pathauto": "^1.8",
    "drupal/queue_mail": "^1.3",
    "drupal/rabbit_hole": "^1.0@beta",
    "drupal/redirect": "^1.6",
    "drupal/roleassign": "^1.0@beta",
    "drupal/search_api": "^1.18",
    "drupal/sitemap": "^2.0",
    "drupal/swiftmailer": "^2.0",
    "drupal/system_status": "^2.9",
    "drupal/time_range": "^9.0",
    "drupal/token": "^1.7",
    "drupal/twig_field_value": "^2",
    "drupal/twig_tweak": "^2.6",
    "drupal/twigsuggest": "^1.0@beta",
    "drupal/ultimate_cron": "^2.0@alpha",
    "drupal/views_data_export": "^1.0",
    "drupal/viewsreference": "^2.0",
    "drupal/vppr": "^1.1",
    "drupal/webform": "^6",
    "drupal/xero": "2.x-dev",
    "drush/drush": "^10.3",
    "furf/jquery-ui-touch-punch": "dev-master",
    "twbs/bootstrap": "^4.5",
    "webflo/drupal-finder": "^1.0.0"
  },
  "require-dev": {
     "drupal/core-dev": "^9"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
      "DrupalProject\\composer\\ScriptHandler::removeFiles",
      "DrupalProject\\composer\\ScriptHandler::moveBootstrap"
    ]
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      }
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
    },
    "drupal-core-project-message": {
      "include-keys": ["homepage", "support"],
      "post-create-project-cmd-message": [
        "<bg=blue;fg=white>                                                         </>",
        "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
        "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
        "<bg=blue;fg=white>                                                         </>",
        "",
        "<bg=yellow;fg=black>Next steps</>:",

        "  * Install the site: https://www.drupal.org/docs/8/install",
        "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
        "  * Get support: https://www.drupal.org/support",
        "  * Get involved with the Drupal community:",
        "      https://www.drupal.org/getting-involved",
        "  * Remove the plugin that prints this message:",
        "      composer remove drupal/core-project-message"
      ]
    }
  }
}